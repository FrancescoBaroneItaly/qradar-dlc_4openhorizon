{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME for $ARCH",
    "description": "",
    "public": true,
    "documentation": "",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
        {
            "name": "",
            "label": "",
            "type": "",
            "defaultValue": ""
        }
    ],
    "deployment": {
        "services": {
            "qradar-dlc": {
                "image": "${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION",
                "privileged": false,
                "ports": [
                    {
                        "HostPort": "1514:1514/tcp",
                        "HostIP": "0.0.0.0"
                    },
					{
                        "HostPort": "1514:1514/udp",
                        "HostIP": "0.0.0.0"
                    }
                    ],
				"binds": ["/server/app/qradar-dlc/data/persistent_queue:/store/persistent_queue:rw"]
            }
        }
    }
}